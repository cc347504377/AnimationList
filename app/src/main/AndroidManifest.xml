<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.whr.myapplication">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".PathEffectActivity">
        </activity>
        <activity android:name=".BezierActivity">
        </activity>
        <activity android:name=".WavyActivity">
        </activity>
        <activity android:name=".CircleBezierActivity">
        </activity>
        <activity android:name=".TransitionActivity">
        </activity>
        <activity android:name=".ToTransitionActivity">
        </activity>
    </application>

</manifest>