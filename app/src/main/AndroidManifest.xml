<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.whr.myapplication">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.PathEffectActivity">
        </activity>
        <activity android:name=".activity.BezierActivity">
        </activity>
        <activity android:name=".activity.WavyActivity">
        </activity>
        <activity android:name=".activity.CircleBezierActivity">
        </activity>
        <activity android:name=".activity.TransitionActivity">
        </activity>
        <activity android:name=".activity.SceneActivity">
        </activity>
        <activity android:name=".activity.VectorActivity">
        </activity>
        <activity
            android:name=".activity.SharedElementActivity"
            android:theme="@style/AnimationTheme">
        </activity>
        <activity
            android:name=".activity.SharedElementActivity2"
            android:theme="@style/AnimationTheme">
        </activity>
        <activity android:name=".activity.ShadowActivity">
        </activity>
        <activity android:name=".activity.Main2Activity">
        </activity>
    </application>

</manifest>